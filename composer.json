    "keywords": ["cache", "redis", "mongodb", "memcached", "rmzamora fork"],
    "homepage": "https://github.com/sonata-project/cache",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mell M. Zamora",
            "email": "me@mellzamora.com",
            "homepage": "http://mellzamora.com"
        },
